<%= render 'errors' %>

<%= form_for @account, url: login_path, method: :post do |f| %>
  <div class="field_with_errors">
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div class="field_with_errors">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div class="actions">
    <%= f.submit "Log In" %>
  </div>

<% end %>
